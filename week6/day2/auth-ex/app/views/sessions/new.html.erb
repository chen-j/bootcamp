<h1> Login </h1>
<%= form_tag "/login" do %>
    <div class="">
        <%= label_tag :email %>
        <%= email_field_tag :email %>
    </div>
    <div class="">
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </div>

    <div class="">
        <%= button_tag "Login" %>
    </div>
<% end %>
